            @Html.DisplayFor(modelItem => item.Region)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.vec1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.vec2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
